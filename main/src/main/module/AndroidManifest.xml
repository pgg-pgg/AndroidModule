<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="pgg.main">

    <application android:allowBackup="true"
        android:name="debug.MainApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/main_app_name" android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name="pgg.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pgg.main.BottomNavigationActivity" android:label="@string/main_title_activity_fragment_module"/>
    </application>

</manifest>